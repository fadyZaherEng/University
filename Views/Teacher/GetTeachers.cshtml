
@{
    ViewData["Title"] = "GetTeachers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
 
    <div id="products" class="row list-group">
        @foreach (Teacher item in Model)
        {
            <div class="item  col-xs-4 col-lg-4">
                <div class="thumbnail">
                <img class="group list-group-image" src="http://placehold.it/400x250/000/fff" alt="" />
                <div class="caption">
                    <h4 class="group inner list-group-item-heading"> @item.TeacherName</h4>
                    <p class="group inner list-group-item-text">Techers</p>
                    <div class="row">
                        <div class="col-xs-12 col-md-6">
                            <p class="lead">$@item.TeacherNumber </p>
                        </div>
                        <div class="col-xs-12 col-md-6">
                            <a class="btn btn-success" href="http://www.jquery2dotnet.com">
                                Details    
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        }
    </div>
</div>




