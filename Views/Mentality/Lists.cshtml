<html lang="en">
 <head>
     <meta charset="utf-8">
     <title>Lists</title>
     <meta name="viewport" content="width=device-width, initial-scale=1">
     <meta name="description" content="Lists">
 </head>
 <body>
     <!-- Unordered List -->
     <h1>Unordered List</h1>
     <ul>
         <li>Item 1</li>
         <li>Item 2</li>
         <li>Item 3</li>
     </ul>
     <!-- Ordered List -->
     <h1>Ordered List</h1>
     <ol>
         <li>Item 1</li>
         <li>Item 2</li>
         <li>Item 3</li>
     </ol>
     <!-- Description List -->
     <h1>Description List</h1>
     <dl>
         <dt>Item 1</dt>
         <dd>Item 1 description</dd>
         <dt>Item 2</dt>
         <dd>Item 2 description</dd>
         <dt>Item 3</dt>
         <dd>Item 3 description</dd>
     </dl>
     <!-- Nested List -->
     <h1>Nested List</h1>
     <ul>
         <li>Item 1
             <ul>
                 <li>Item 1.1</li>
                 <li>Item 1.2</li>
             </ul>
         </li>
         <li>Item 2
             <ul>
                 <li>Item 2.1</li>
                 <li>Item 2.2</li>
             </ul>
         </li>Item 3
         <ol>
             <li>Item 3.1</li>
             <li>Item 3.2</li>
         </ol>
     </ul>

 </body>
</html>
